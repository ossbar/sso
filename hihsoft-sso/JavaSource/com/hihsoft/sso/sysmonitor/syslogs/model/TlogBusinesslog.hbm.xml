<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hihsoft.sso.sysmonitor.syslogs.model">
	<class name="TlogBusinesslog" table="T_LOG_BUSINESSLOG">

		<id name="logid" column="LOGID" type="java.lang.String" length="32">
			<generator class="uuid"/>
		</id>
		
		
		
		<property 
			name="createDate" 
			column="CREATE_DATE" 
			update="true"
			insert="true"
			type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="30"/>
		
		<property 
			name="custip" 
			column="CUSTIP" 
			update="true"
			insert="true"
			type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="15"/>
		
		<property 
			name="moduleid" 
			column="MODULEID" 
			update="true"
			insert="true"
			type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="operateid" 
			column="OPERATEID" 
			update="true"
			insert="true"
			type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="orgid" 
			column="ORGID" 
			update="true"
			insert="true"
			type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="userid" 
			column="USERID" 
			update="true"
			insert="true"
			type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<!-- One-To-Many attributes -->

		<!-- Many-To-One attributes -->
		
		<many-to-one
			name="tsysModuleoperate"
			not-null="false"
			update="false"
			insert="false"
			cascade="none"
			>	
			<column name="OPERATEID"/>
		</many-to-one> 
		
		<many-to-one
			name="tsysModuleinfo"
			not-null="false"
			update="false"
			insert="false"
			cascade="none"
			>	
			<column name="MODULEID"/>
		</many-to-one> 
		
		<many-to-one
			name="taclUserinfo"
			not-null="false"
			update="false"
			insert="false"
			cascade="none"
			>	
			<column name="USERID"/>
		</many-to-one> 
		
		<many-to-one
			name="tsysOrg"
			not-null="false"
			update="false"
			insert="false"
			cascade="none"
			>	
			<column name="ORGID"/>
		</many-to-one> 

	</class>
</hibernate-mapping>

